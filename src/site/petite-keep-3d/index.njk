{% extends 'layouts/experiment.njk' %}
{% block bodyClass %}background--light{% endblock %}
{% block styles %}
  <link rel="stylesheet" href="sketch.css">
{% endblock %}
{% block scripts %}
  <script type="module" src="https://ajax.googleapis.com/ajax/libs/model-viewer/3.2.0/model-viewer.min.js"></script>
  <script src="sketch.js"></script>
{% endblock %}
{% block footer %}{% endblock %}


{% block content %}
  <model-viewer src="Petite Keep 3D.glb" ar ar-modes="webxr scene-viewer quick-look" camera-controls poster="poster.webp" shadow-intensity="1" exposure="1">
    <div class="progress-bar hide" slot="progress-bar">
        <div class="update-bar"></div>
    </div>
    <button slot="ar-button" id="ar-button">
        View in your space
    </button>
    <div id="ar-prompt">
        <img src="ar_hand_prompt.png">
    </div>
  </model-viewer>
  <script src="sketch.js"></script>
{% endblock %}
